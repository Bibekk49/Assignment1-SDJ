<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.rbr.RBRController">
    <children>
        <VBox alignment="CENTER" layoutX="-1.0" prefHeight="400.0"
              prefWidth="600.0" spacing="50.0">
            <children>
                <Button alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" onAction="#reserveVinyl"
                        prefHeight="43.0"
                        prefWidth="293.0" text="Reserve" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" prefHeight="43.0"
                        onAction="#borrowVinyl"
                        prefWidth="293.0" text="Borrow" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" prefHeight="43.0"
                        onAction="#returnVinyl"
                        prefWidth="293.0" text="Return" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Button>
                <Button alignment="CENTER" contentDisplay="CENTER"
                        mnemonicParsing="false" prefHeight="43.0"
                        onAction="#back"
                        prefWidth="118.0" text="Back" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
