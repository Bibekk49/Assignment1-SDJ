            <TableColumn fx:id="year" prefWidth="75.0" text="Year" />
            <TableColumn fx:id="state" prefWidth="75.0" text="State" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" layoutX="-1.0" layoutY="397.0" prefHeight="50.0" prefWidth="610.0" spacing="300.0">
